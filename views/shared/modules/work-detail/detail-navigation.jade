        .four.columns.offset-by-one.nav.right
          .work-nav-btn-wrap
            a(href="#NEXT").work-nav-btn
              span.label-wrap
                +svgIcon('arrow-right', 'white size-16')
                span.t3.t3-marginless.t3-light.label View Next Project
