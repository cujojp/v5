include ../../../../views/shared/global/jade-helpers.jade
- dataObj = data;

section.detail-navigation
  .nav-wrap
    .container
      .row
        .six.columns.offset-by-one.nav.left
          .work-nav-btn-wrap
            a(href="/work").work-nav-btn
              span.label-wrap
                +svgIcon('keyboard', 'white size-16')
